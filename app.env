

# ==============================================================================
# Spring Boot 应用与 AI 服务配置
# ==============================================================================

# 应用名称
SPRING_APPLICATION_NAME=demo_ai_2

# AI 服务配置 (阿里云通义千问)
SPRING_AI_OPENAI_BASE_URL=https://dashscope.aliyuncs.com/compatible-mode
SPRING_AI_OPENAI_API_KEY=your-actual-api-key  # 请替换为您的真实API密钥

# Chat 模型配置
SPRING_AI_OPENAI_CHAT_OPTIONS_MODEL=qwen-omni-turbo
SPRING_AI_OPENAI_CHAT_OPTIONS_TEMPERATURE=0.8

# Embedding 模型配置
SPRING_AI_OPENAI_EMBEDDING_BASE_URL=https://dashscope.aliyuncs.com/compatible-mode
SPRING_AI_OPENAI_EMBEDDING_API_KEY=your-actual-api-key  # 请替换为您的真实API密钥
SPRING_AI_OPENAI_EMBEDDING_OPTIONS_MODEL=text-embedding-v3
SPRING_AI_OPENAI_EMBEDDING_OPTIONS_DIMENSIONS=1024


# ==============================================================================
# Servlet 配置
# ==============================================================================

# 文件上传大小限制
SPRING_SERVLET_MULTIPART_MAX_FILE_SIZE=20MB
SPRING_SERVLET_MULTIPART_MAX_REQUEST_SIZE=20MB


# ==============================================================================
# 数据源配置
# ==============================================================================

# 数据库驱动
SPRING_DATASOURCE_DRIVER_CLASS_NAME=com.mysql.cj.jdbc.Driver

# 数据库连接URL
# 注意：URL中的特殊字符和参数已经包含在内
SPRING_DATASOURCE_URL=jdbc:mysql://localhost:3306/itheima?serverTimezone=Asia/Shanghai&useSSL=false&useUnicode=true&characterEncoding=utf-8&zeroDateTimeBehavior=convertToNull&transformedBitIsBoolean=true&tinyInt1isBit=false&allowPublicKeyRetrieval=true&allowMultiQueries=true&useServerPrepStmts=false

# 数据库用户名和密码
SPRING_DATASOURCE_USERNAME=root
SPRING_DATASOURCE_PASSWORD=123456


# ==============================================================================
# 日志配置
# ==============================================================================

# 日志级别
LOGGING_LEVEL_ORG_SPRINGFRAMEWORK_AI=DEBUG
LOGGING_LEVEL_COM_EXAMPLE_DEMO=DEBUG
